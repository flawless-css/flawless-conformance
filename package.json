{
  "name": "flawless-conformance",
  "version": "0.0.0",
  "description": "Conformance testing for less",
  "main": "dist/index.js",
  "scripts": {
    "test": "tape ./spec/*.js",
    "build": "rm -rf dist && babel lib --out-dir dist",
    "watch": "babel --watch lib --out-dir dist",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/flawless/flawless-conformance.git"
  },
  "keywords": [
    "less",
    "flawless",
    "conformance",
    "preprocessor",
    "css",
    "lint"
  ],
  "author": "Matt Styles",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/flawless/flawless-conformance/issues"
  },
  "homepage": "https://github.com/flawless/flawless-conformance",
  "dependencies": {
    "chalk": "^1.0.0",
    "core-js": "^0.8.3",
    "less": "^2.5.0",
    "minimist": "^1.1.1"
  },
  "devDependencies": {
    "babel": "^5.1.8"
  }
}
